      $(window).load(function() {
        setTimeout(function() {
          $('#loading').fadeOut(400, "linear");
        }, 300);
      });
    </script>
    <style>
      #loading {
        position: fixed;
        width: 100%;
        height: 100%;
        left: 0;
        top: 0;
        right: 0;
        bottom: 0;
        display: block;
        background: #fff;
        z-index: 10000;
      }

      #loading img {
        position: absolute;
        top: 50%;
        left: 50%;
        margin: -23px 0 0 -23px;
      }
    </style>
